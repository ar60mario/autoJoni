<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.ventas.entities.ConfiguracionTop" schema="joni" table="configuracion_top">
        <id name="id" column="ID">
            <generator class="identity"/>
        </id>
        <property name="orderMaximo" type="integer" column="ORDER_MAXIMO"/>
        <property name="importeMaximo" type="double" column="IMPORTE_MAXIMO"/>
        <property name="importeMaximoPanificados" type="double" column="IMPORTE_MAXIMO_PANIFICADOS"/>
        <property name="importeMinimoMercadoPago" type="double" column="IMPORTE_MINIMO_MERCADO_PAGO"/>
    </class>
</hibernate-mapping>